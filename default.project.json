{
    "name": "Aet",
    "globIgnorePaths": [
        "**/package.json",
        "**/tsconfig.json",
        "**/.pesde",
        "**/pesde.toml",
        "**/pesde.lock",
        "**/wally.toml",
        "**/*.md"
    ],
    "tree": {
        "$className": "Actor",
        "Aet": {
            "$path": "out",
            "include": {
                "$className": "Folder",
                "$path": "include",
                "node_modules": {
                    "$className": "Folder",
                    "@rbxts": {
                        "$path": "node_modules/@rbxts"
                    }
                }
            }
        }
    }
}
